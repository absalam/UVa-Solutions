#include <stdio.h>

int main()
{
    long long hsn, osn, temp, res;

    while(scanf("%lld %lld", &hsn, &osn) != EOF) {
        if(hsn>osn) {
            temp=hsn;
            hsn=osn;
            osn=temp;
        }
        res=osn-hsn;
        printf("%lld\n", res);
    }
    return 0;
}
